{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "australiaeast"
    },
    "namePrefix": {
      "value": "alpacamock"
    },
    "environment": {
      "value": "prod"
    },
    "polygonApiKey": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/<subscription-id>/resourceGroups/<resource-group>/providers/Microsoft.KeyVault/vaults/<keyvault-name>"
        },
        "secretName": "polygon-api-key"
      }
    },
    "apiKey": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/<subscription-id>/resourceGroups/<resource-group>/providers/Microsoft.KeyVault/vaults/<keyvault-name>"
        },
        "secretName": "alpacamock-api-key"
      }
    },
    "apiSecret": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/<subscription-id>/resourceGroups/<resource-group>/providers/Microsoft.KeyVault/vaults/<keyvault-name>"
        },
        "secretName": "alpacamock-api-secret"
      }
    },
    "postgresPassword": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/<subscription-id>/resourceGroups/<resource-group>/providers/Microsoft.KeyVault/vaults/<keyvault-name>"
        },
        "secretName": "alpacamock-postgres-password"
      }
    },
    "ingestionCronSchedule": {
      "value": "0 6 * * *"
    }
  }
}
